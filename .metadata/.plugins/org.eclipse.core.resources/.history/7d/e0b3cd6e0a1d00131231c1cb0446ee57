package com.Fluffeater.semideus.Instance.Particle;

import java.util.Random;

import com.Fluffeater.semideus.Graphics.Sprite;
import com.Fluffeater.semideus.Graphics.SpriteSheet;

public class BloodSplatter extends Particle{
	/* 
	 * Copyright Endoplasm Gaming 2013
	 * Do not distribute
	 */
	
	public BloodSplatter(int x, int y){
		super(x,y);
		Random random = new Random();
		sprite = new Sprite(6, 6, 6, 0, 3, 3, SpriteSheet.blood);
	}
	
}
